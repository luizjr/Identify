{
    "name": "luizjr/identify",
    "description": "A Laravel 6 Package Provider to Identify/detect a user's browser, device, operating system and Language",
    "keywords": [
      "github",
      "laravel",
      "Open Source",
      "Detect",
      "Browser",
      "Operating System",
      "Language"
    ],
    "license": "MIT",
    "authors": [
      {
        "name": "LuizJr",
        "email": "luizjrdeveloper@gmail.com"
      }
   ],
   "minimum-stability": "stable",
   "require": {
    "php": ">=5.5.9",
    "illuminate/support": "^5.0|^6.0",
    "sinergi/browser-detector": "6.*"
   },
   "autoload": {
        "psr-4": {
          "LuizJr\\Identify\\": "src/"
        }
   },
   "autoload-dev": {
        "psr-4": {
            "LuizJr\\Identify\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
